<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Attendance_v10" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#27479A</item>
        <item name="colorPrimaryVariant">#27479A</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#090808</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#27479A</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Attendance_v10.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Attendance_v10.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Attendance_v10.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>